  text-decoration: underline;
}

.question.active {
  color: green;
}

.question.active:focus {
  outline: none;
}

.question.active + .answer a {
  display: block;
  text-align: center;
  margin-top: 10px;
}

.question.active + .answer a:visited {
  color: green;
}

.question.active + .answer a:hover {
  text-decoration: underline;
}

